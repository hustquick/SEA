	Journal = {Simulation Modelling Practice and Theory},
	Keywords = {Thermodynamic model},
	Note = {Sustainable Energy and Environmental Protection ``SEEP2009''},
	Number = {4},
	Pages = {1227 - 1234},
	Title = {Development of a theoretical decoupled Stirling cycle engine},
	Url = {http://www.sciencedirect.com/science/article/pii/S1569190X10001322},
	Volume = {19},
	Year = {2011},
