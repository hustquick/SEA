	Volume = {108},
	Year = {2016},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S1359431116313606},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.applthermaleng.2016.08.014}}

